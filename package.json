{
  "name": "vite-vue-typescript-starter",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon -w src/server -x tsx src/server/main.ts",
    "start": "NODE_ENV=production tsx src/server/main.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "pnpm lint:fix && vite build",
    "prepare": "husky"
  },
  "dependencies": {
    "@types/node-cron": "^3.0.11",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "express": "^4.18.2",
    "install": "^0.13.0",
    "mysql2": "^3.10.1",
    "node-cron": "^3.0.3",
    "protobufjs": "^7.3.2",
    "redis": "^4.6.15",
    "sequelize": "^6.37.3",
    "tsx": "^4.4.0",
    "typescript": "^5.3.2",
    "vite-express": "*",
    "vue": "^3.3.8",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.3",
    "@vitejs/plugin-vue": "^4.5.0",
    "eslint": "9.4.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.0.1",
    "vite": "^5.0.2",
    "vue-tsc": "^1.8.22"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint . --fix"
    ]
  }
}
